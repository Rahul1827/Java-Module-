

package Pattern;

public class Pattern6 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		
		int row=5;
		int col=5;
		
		for(int i=1;i<=5;i++)
			
		{
			
			for(int j =0;j<=i;j++)
				
			{
				
				
				System.out.print(+i);
			}
			System.out.println();
		}

	}

}
