package Pattern;

public class AllPatern {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int row = 5;
		int col = 5;

		for (int i = 0; i < 5; i++) {

			for (int j = 0; j < 5; j++) {

				System.out.print("  *  ");
			}

			System.out.println();
		}

		System.out.println("********************************************************************");
		for (int i = 1; i <= 5; i++) {

			for (int j = 1; j <= 5; j++) {

				System.out.print("    " + i);
			}

			System.out.println();
		}

	}

}
