package Pattern;
import java.util.Scanner;


public class Pattern10 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Scanner sc=new Scanner(System.in);
		
		int row =sc.nextInt();
		int col=sc.nextInt();

		for (int i = 0; i <row; i++)

		{

			for (int j = 0; j < col; j++)

			{
if(i==2 || j==2)
{
	System.out.print("R");

}

else {
				
				System.out.print(" ");
			}
			}
			System.out.println();
		}

	}

}
