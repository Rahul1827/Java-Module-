package Pattern;

public class Pattern8 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int row = 5;
		int col = 1;

		for (int i = 1; i <row; i++)

		{

			for (int j = 1; j <col; ++)

			{

				System.out.print(i%2);
			}
			System.out.println();
		}


	}

}
