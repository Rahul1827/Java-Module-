package Pattern;

public class Pattern5 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int row = 5;
		int col = 5;

		for (int i = 65; i <= row; i++) {

			for (int j = 65; j <= col; j++) {

				System.out.print("   " + i);
			}

			System.out.println();
		}

	}

}

