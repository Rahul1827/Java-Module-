



	package Pattern;

	public class Pattern4 {

		public static void main(String[] args) {
			// TODO Auto-generated method stub
			
			
		char row=5;
			char col=5;
			
			for(char i=1;i<=5;i++)
			{
				
				for(char j=1; j<=5;j++)
				{
					
					
					System.out.print("    "+(i+j)%2);
				}
				
				System.out.println();
			}

		}

	


	}


