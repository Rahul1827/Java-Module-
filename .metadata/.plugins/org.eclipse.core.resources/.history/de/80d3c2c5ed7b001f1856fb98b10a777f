package module3;

public class WhileDemo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
	int count =0;
	int num=1;
	
	while(count<10)
	{
		
		System.out.println(num);
		num=num+2;
		count++;
	}
	
	
	
	System.out.println("****************************************************");
	
	
	
	int c=0;
	int n=1;
	
	while(n%2==0)
	{
		System.out.println(n);
		c++;
		
		
	}
	
	}
	
	

}

